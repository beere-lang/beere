import beere.lib.std as std

public class Main {
	private static count: int = 0

	public static fn increment(): void {
		count++
	}

	public static fn get(): int {
		return count
	}

	public static fn reset(): void {
		count = 0
	}
}

public fn main(): void {
	std.println("Hello, World!")

	let main: Main = new Main()
	let x: int = main.increment()
}
