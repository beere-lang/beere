import "../utils/logger/logger.beere" as logger

public class Main {
	private count: int = 0

	public fn increment(): void {
		this.count++
	}

	public fn get(): int {
		return this.count
	}

	public fn reset(): void {
		this.count = 0
	}
}

private fn main(): void {
	let main: Main = new Main()
	main.increment()

	let x: int = main.get()

	logger.info("Count: {}", x)
}
