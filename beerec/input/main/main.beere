import beere.lib.std as std

public class Main {
	private count: int = 0

	public fn increment(): void {
		count++
	}

	public fn get(): int {
		return count
	}

	public fn reset(): void {
		count = 0
	}
}

public fn main(): void {
	std.println("Hello, World!")

	let main: Main = new Main()
	main.increment()

	let x: int = main.get()

	std.printlnf("Count: {}", x)
}
